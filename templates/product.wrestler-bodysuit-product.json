          "settings": {
            "custom_liquid": "{%- if product.description != blank -%}\n<div id=\"bt-head\">{{ product.description | strip_html | truncatewords: 20}}<\/div>\n    <div id=\"dots\">...<\/div>\n     <div id=\"more\">{{ product.description }}<\/div>\n<div id=\"myBtnn\" class=\"\" style=\"font-weight: 600;\">Read more<\/div>\n\n<style>\n  div#more {\n    display: none;\n}\n  div#dots {\n    width: 0;\n    height: 0;\n    opacity: 0;\nmargin:0;\n    visibility: hidden;\n}\n<\/style>\n<script>\n  $(\"#myBtnn\").on(\"click\", function(){\n    if( $(\"#dots\").css(\"display\") == \"none\" ){\n      $(\"#dots\").show();\n      $(\"#myBtnn\").text(\"Read more\");\n      $(\"#more\").hide();\n      $(\"#bt-head\").show();\n    }\n    else{\n      $(\"#dots\").hide();\n      $(\"#myBtnn\").text(\"Read less\");\n      $(\"#more\").show();\n      $(\"#bt-head\").hide();\n    }\n  });\n<\/script>\n{%- endif -%}"
          }
