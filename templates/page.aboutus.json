    "6ecaf23b-e66a-4a7b-8cca-07a78ae86a2b": {
      "type": "image-with-text",
      "disabled": true,
      "blocks": {
        "template--18863682552109__6ecaf23b-e66a-4a7b-8cca-07a78ae86a2b-16825816007eaec60e-0": {
          "type": "heading",
